<?xml version="1.0"?>

<PropertyList>
    <generic>
        <output>
            <line_separator>newline</line_separator>
            <var_separator>,</var_separator>
            <chunk>
                <name>IAS</name>
                <type>float</type>
                <format>%.1f</format>
                <node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
            </chunk>
            <chunk>
                <name>PITCH</name>
                <type>float</type>
                <format>%.1f</format>
                <node>/orientation/pitch-deg</node>
            </chunk>
            <chunk>
                <name>ROLL</name>
                <type>float</type>
                <format>%.1f</format>
                <node>/orientation/roll-deg</node>
            </chunk>
            <chunk>
                <name>HDG</name>
                <type>float</type>
                <format>%.1f</format>
                <node>/orientation/heading-magnetic-deg</node>
            </chunk>
            <chunk>
                <name>ALT</name>
                <type>ing</type>
                <format>%d</format>
                <node>/instrumentation/altimeter/pressure-alt-ft</node>
            </chunk>
            <chunk>
                <name>TIME</name>
                <type>string</type>
                <format>%s</format>
                <node>/instrumentation/clock/indicated-string</node>
            </chunk>
            <chunk>
                <name>VOLT</name>
                <type>float</type>
                <format>%.4e</format>
                <node>/systems/electrical/volts</node>
            </chunk>
            <chunk>
                <name>RPM</name>
                <type>int</type>
                <format>%d</format>
                <node>/engines/engine/rpm</node>
            </chunk>
            <chunk>
                <name>MAP</name>
                <type>float</type>
                <format>%.1f</format>
                <node>/engines/engine/mp-inhg</node>
            </chunk>
            <chunk>
                <name>OILT</name>
                <type>float</type>
                <format>%.1f</format>
                <node>/engines/engine[0]/oil-temperature-degf</node>
            </chunk>
            <chunk>
                <name>OILP</name>
                <type>float</type>
                <format>%.1f</format>
                <node>/engines/engine[0]/oil-pressure-psi</node>
            </chunk>
            <chunk>
                <name>EGT</name>
                <type>double</type>
                <format>%.4e</format>
                <node>/engines/engine/egt-degf-fix</node>
            </chunk>
            <chunk>
                <name>FFLOW</name>
                <type>double</type>
                <format>%.4e</format>
                <node>/engines/engine/fuel-flow-gph</node>
            </chunk>
            <chunk>
                <name>FUEL0</name>
                <type>float</type>
                <format>%.2f</format>
                <node>/consumables/fuel/tank[0]/level-gal_us</node>
            </chunk>
            <chunk>
                <name>FUEL1</name>
                <type>float</type>
                <format>%.2f</format>
                <node>/consumables/fuel/tank[1]/level-gal_us</node>
            </chunk>
            <chunk>
                <name>LAT</name>
                <type>float</type>
                <format>%.4e</format>
                <node>/instrumentation/gps/indicated-latitude-deg</node>
            </chunk>
            <chunk>
                <name>LONG</name>
                <type>float</type>
                <format>%.4e</format>
                <node>/instrumentation/gps/indicated-longitude-deg</node>
            </chunk>
            <chunk>
                <name>OAT</name>
                <type>float</type>
                <format>%.1f</format>
                <node>/environment/temperature-degc</node>
            </chunk>
        </output>
        <input>
            <line_separator>newline</line_separator>
            <var_separator>,</var_separator>
            <chunk>
                <name>ELEV</name>
                <type>double</type>
                <format>%.4f</format>
                <node>/controls/flight/elevator</node>
            </chunk>
            <chunk>
                <name>AILR</name>
                <type>double</type>
                <format>%.4f</format>
                <node>/controls/flight/aileron</node>
            </chunk>
            <chunk>
                <name>RUD</name>
                <type>double</type>
                <format>%.4f</format>
                <node>/controls/flight/rudder</node>
            </chunk>
            <chunk>
                <name>FLAPS</name>
                <type>double</type>
                <format>%.4f</format>
                <node>/controls/flight/flaps</node>
            </chunk>
            <chunk>
                <name>THR</name>
                <type>double</type>
                <format>%.4f</format>
                <node>/controls/engines/engine/throttle</node>
            </chunk>
        </input>
    </generic>
</PropertyList>
